
Spenat{
  name: mySpenat;

  //commenting something//
  Port p_in;

  /*or commenting something
    else
    sfsdfdsfdsfdsfdsf */
  Place
  {
    name: p1;
    initial: true;
  }

  Place{name: p2;}
  Place{name: p3;}
  Place{name: p4;}

  SignalTransition{
    name: t1;

    pre: p1;
    post: p2, p3;
  }

  SignalTransition{
    name: t2;

    pre: p2, p3;
    post: p4;
  }
}
